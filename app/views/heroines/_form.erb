<% if flash[:error] %>
  <% flash[:error].each do |msg|  %>
    <%= msg %>
  <% end %>
<% end %>
<br><br>

<%= form_for @heroine do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :super_name %>
  <%= f.text_field :super_name %>

  <%= f.label :power_id %>
  <%= f.select :power_id, @powers %>

  <%= f.submit %>

<% end  %>

<br>
<br>
<%= link_to "Back to Index Page", heroines_path %>
