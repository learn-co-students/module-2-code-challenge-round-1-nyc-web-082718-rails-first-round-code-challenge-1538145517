<% if flash[:error] %>
  <% flash[:error].each do |e| %>
    <%= e %>
  <% end %>
<% end %>

<%= form_for @heroine do |f| %>
<%= f.label "Name" %>
<%= f.text_field :name %>
<%= f.label "Super Name" %>
<%= f.text_field :super_name %>
<%= f.label "Choose a power" %>
<%= collection_select :heroine, :power_id, Power.all, :id, :name %>
<%= f.submit "Submit Heroine" %>
<% end %>
