<% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
    <%= error %>
  <% end %>
<% end %>
<%= form_for @heroine do |f| %>
<%= f.label :super_name %>
<%= f.text_field :super_name %></br>
<%= f.label :name %>
<%= f.text_field :name %></br>
<%= f.label :power_id %>
<%= f.select :power_id, @power_options %></br>
<%= f.submit %>
<% end %>
