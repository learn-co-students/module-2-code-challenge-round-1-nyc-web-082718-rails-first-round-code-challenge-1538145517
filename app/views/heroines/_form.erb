<% if flash[:errors] %>
<% flash[:errors].each do |msg| %>
<ul>
  <li> <%=  msg%></li>
</ul>
<% end %>
<% end %>

<%= form_for @heroine do |f| %>
<%= f.label :name %><br>
<%= f.text_field :name %><br>

<%= f.label :super_name %><br>
<%= f.text_field :super_name %><br>

<%= f.label "Power" %><br>
<%= f.collection_select :power_id, Power.all, :id, :name %><br>

<%= f.submit %>
<% end %>
