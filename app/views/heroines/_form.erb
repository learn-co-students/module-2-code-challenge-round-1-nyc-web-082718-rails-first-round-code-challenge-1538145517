<% if flash[:error] %>
  <% flash[:error].each do |e| %>
    <%= e %><br>
  <% end %>
<% end %>
<br>
<%= form_for @heroine do |f| %>
  <b><%= f.label :name %></b><br>
    <%= f.text_field :name %><br><br>
  <b><%= f.label :super_name %></b><br>
    <%= f.text_field :super_name %><br><br>
  <b><%= f.label :Power %></b><br>
    <%= collection_select :heroine, :power_id, Power.all, :id, :name %><br><br>
  <%= f.submit %>
<% end %>
